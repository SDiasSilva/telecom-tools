    <EmbeddedResource Remove="Model\Ef\**" />
    <None Remove="Controller\Ef\**" />
    <None Remove="Ef\**" />
    <None Remove="Model\Ef\**" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Controller\" />
    <Folder Include="Model\" />
  </ItemGroup>

</Project>