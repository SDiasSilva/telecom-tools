    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Controller\Ef\**" />
    <Compile Remove="Ef\**" />
    <Compile Remove="Model\Ef\**" />
